  "query": {
    "$query": {
      "$or": [
        {
          "keyword": "UK Black Pride",
          "keywordLoc": "title,body"
        },
        {
          "keyword": "Black LGBTQ Britain",
          "keywordLoc": "title,body"
        },
        {
          "keyword": "Black queer UK",
          "keywordLoc": "title,body"
        }
      ],
